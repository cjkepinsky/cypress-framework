{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom"],
    "types": ["cypress", "node", "cypress-real-events"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "baseUrl": "./",
    "paths": {
      "config/*": ["cypress/config/*"],
      "helpers/*": ["cypress/helpers/*"],
      "pages/*": ["cypress/pages/*"],
      "routes/*": ["cypress/routes/*"]
    }
  },
  "include": ["cypress/**/*.ts"]
}